<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="205.0" prefWidth="428.0" spacing="20.0" style="-fx-background-color: #f4f4f4;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
  <Label fx:id="welcomeText" /> 
  <Button fx:id="loginPageButton" alignment="CENTER" contentDisplay="CENTER" onAction="#onGotoLoginPageButtonClick" style="-fx-background-radius: 500; -fx-background-color: #23cf92aa; -fx-pref-width: 300; -fx-pref-height: 50;" text="Go to Login Page" textAlignment="CENTER" />

  <Button fx:id="dashboardButton" alignment="CENTER" contentDisplay="CENTER" onAction="#onGotoDashboardButtonClick" style="-fx-background-radius: 500; -fx-background-color: #1baafdaa; -fx-pref-width: 300; -fx-pref-height: 50;" text="Go to Dashboard" textAlignment="CENTER" />
</VBox>
